{
  // Set the opacity for the vibrancy effect in VSCode window
  "vscode_vibrancy.opacity": 0.85,
  // Specify custom theme to be used for vibrancy effect
  "vscode_vibrancy.theme": "Custom theme (use imports)",
  // Set preferred dark theme for vibrancy
  "vscode_vibrancy.preferredDarkTheme": "Custom theme (use imports)",
  // Refresh interval for updating vibrancy effect
  "vscode_vibrancy.refreshInterval": 100,

  // Editor settings for customization
  "editor.cursorStyle": "block-outline", // Use block-outline style for cursor
  "editor.columnSelection": true, // Enable column selection mode
  "editor.renderWhitespace": "all", // Render all whitespace characters
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true, // Assign unique colors to each bracket type
  "editor.codeActions.triggerOnFocusChange": true, // Trigger code actions when focus changes
  "editor.definitionLinkOpensInPeek": true, // Open definitions in peek view without navigating
  "editor.experimentalEditContextEnabled": true, // Enable experimental edit context feature
  "editor.experimentalInlineEdit.enabled": true, // Enable inline edit feature
  "editor.experimentalInlineEdit.showToolbar": "always", // Always show toolbar during inline editing
  "editor.inlayHints.padding": true, // Add padding for inlay hints
  "editor.inlineCompletionsAccessibilityVerbose": true, // Enable verbose accessibility for inline completions
  "editor.linkedEditing": true, // Enable linked editing for matching tags
  "editor.padding.top": 25, // Add top padding in the editor
  "editor.padding.bottom": 25, // Add bottom padding in the editor
  "editor.guides.bracketPairs": "active", // Show bracket pair guides for active brackets
  "editor.renderLineHighlightOnlyWhenFocus": true, // Render line highlight only when focused
  "editor.scrollbar.horizontal": "visible", // Always show horizontal scrollbar
  "editor.scrollbar.scrollByPage": true, // Enable scrolling by page increment
  "editor.smoothScrolling": true, // Enable smooth scrolling
  "editor.stablePeek": true, // Keep peek window stable
  "editor.tabCompletion": "on", // Enable tab-based completion
  "editor.suggest.snippetsPreventQuickSuggestions": true, // Prevent snippets from blocking quick suggestions
  "editor.cursorSmoothCaretAnimation": "on", // Enable smooth caret animation
  "editor.inlineSuggest.syntaxHighlightingEnabled": true, // Enable syntax highlighting for inline suggestions
  "editor.suggest.localityBonus": true, // Prioritize suggestions based on locality
  "editor.suggest.preview": true, // Enable preview of suggestions
  "editor.suggest.shareSuggestSelections": true, // Share suggestion selections across sessions
  "editor.suggest.showStatusBar": true, // Show suggestion status bar
  "editor.minimap.autohide": true, // Automatically hide minimap when not in use
  "editor.wordWrap": "on", // Enable word wrapping
  "editor.cursorSurroundingLinesStyle": "all", // Always surround cursor with surrounding lines
  "editor.find.autoFindInSelection": "always", // Automatically enable 'find in selection' mode
  "editor.formatOnType": true, // Format code as you type

  // Workbench settings for customization
  "workbench.editor.enablePreviewFromCodeNavigation": true, // Enable preview when navigating code
  "workbench.editor.enablePreviewFromQuickOpen": true, // Enable preview for quick open
  "workbench.editor.highlightModifiedTabs": true, // Highlight tabs with unsaved changes
  "workbench.colorTheme": "Default Dark+", // Set color theme to Default Dark+
  "workbench.list.smoothScrolling": true, // Enable smooth scrolling for lists
  "workbench.list.scrollByPage": true, // Enable scrolling by page in lists
  "workbench.reduceMotion": "off", // Disable motion reduction
  "workbench.preferredDarkColorTheme": "355d711c-2fa6-4df2-9504-dd44d8f5e350", // Set preferred dark theme ID
  "workbench.view.alwaysShowHeaderActions": true, // Always show header actions
  "workbench.editor.alwaysShowEditorActions": true, // Always show editor actions
  "workbench.editor.centeredLayoutFixedWidth": true, // Enable fixed-width centered layout
  "workbench.iconTheme": "vscode-icons", // Set icon theme to vscode-icons
  "workbench.settings.applyToAllProfiles": [], // Apply settings to all profiles
  "workbench.editor.revealIfOpen": true, // Reveal editor if file is already open
  "workbench.commandPalette.experimental.suggestCommands": true, // Enable command suggestions in command palette
  "workbench.commandCenter": true, // Enable command center at the top
  "workbench.colorCustomizations": {
    "terminal.background": "#00000000" // Set terminal background to transparent
  },

  // Window settings
  "window.titleSeparator": "╰─➤ ", // Set title separator
  "window.newWindowDimensions": "inherit", // Inherit dimensions for new windows
  "window.dialogStyle": "custom", // Use custom dialog style
  "window.doubleClickIconToClose": true, // Enable double-click icon to close window
  "window.systemColorTheme": "dark", // Set system color theme to dark
  "window.menuBarVisibility": "visible", // Always show menu bar
  "window.newWindowProfile": "Default", // Use default profile for new windows
  "window.autoDetectColorScheme": true, // Automatically detect color scheme based on system
  "window.titleBarStyle": "native", // Use native title bar style

  // Terminal settings for customization
  "terminal.integrated.allowMnemonics": true, // Allow mnemonics (Alt-key shortcuts) in terminal
  "terminal.integrated.defaultProfile.windows": "PowerShell", // Set default terminal profile to PowerShell on Windows
  "terminal.integrated.detectLocale": "on", // Enable automatic locale detection
  "terminal.integrated.enableImages": true, // Enable images in terminal output
  "terminal.integrated.enablePersistentSessions": false, // Disable persistent sessions
  "terminal.integrated.enableVisualBell": true, // Enable visual bell for terminal alerts
  "terminal.integrated.environmentChangesIndicator": "on", // Show environment change indicators
  "terminal.integrated.experimental.windowsUseConptyDll": true, // Use experimental ConPTY DLL on Windows
  "terminal.integrated.gpuAcceleration": "on", // Enable GPU acceleration for terminal
  "terminal.integrated.macOptionClickForcesSelection": true, // Allow Option-click to force selection on macOS
  "terminal.integrated.smoothScrolling": true, // Enable smooth scrolling in terminal
  "terminal.integrated.suggest.enabled": true, // Enable suggestion support in terminal
  "terminal.integrated.tabs.defaultColor": "terminal.ansiRed", // Set default tab color to ANSI red
  "terminal.integrated.accessibleViewPreserveCursorPosition": true, // Preserve cursor position in accessible view
  "terminal.integrated.cursorBlinking": true, // Enable cursor blinking in terminal
  "terminal.integrated.copyOnSelection": true, // Automatically copy text when selected
  "terminal.integrated.sendKeybindingsToShell": true, // Send keybindings directly to terminal shell

  // GitLens settings for Git integration
  "gitlens.advanced.fileHistoryShowMergeCommits": true, // Show merge commits in file history
  "gitlens.graph.dimMergeCommits": true, // Dim merge commits in graph
  "gitlens.graph.showRemoteNames": true, // Show remote names in Git graph
  "gitlens.graph.layout": "editor", // Use editor layout for Git graph
  "gitlens.views.repositories.showIncomingActivity": true, // Show incoming activity for repositories
  "gitlens.views.contributors.showAllBranches": true, // Show contributors from all branches
  "gitlens.hovers.currentLine.over": "line", // Show hover information for the current line

  // Screencast mode settings
  "screencastMode.keyboardOptions": {
    "showCommandGroups": true // Show command groups in screencast mode
  },

  // Animation settings
  "animations.Command-Palette": "Slide", // Use slide animation for command palette
  "animations.Active": "Indent", // Use indent animation for active elements
  "animations.Durations": {
    "Command-Palette": 300, // Set duration for command palette animation (ms)
    "Tabs": 200, // Set duration for tab animations (ms)
    "Active": 200, // Set duration for active element animations (ms)
    "Scrolling": 200, // Set duration for scrolling animations (ms)
    "Smooth-Mode": 200, // Set duration for smooth mode animations (ms)
    "Focus-Dimming": 200 // Set duration for focus dimming animations (ms)
  },
  "animations.Scrolling": "Slide", // Use slide animation for scrolling
  "animations.Tabs": "Slide", // Use slide animation for tabs
  "animations.Install-Method": "Custom CSS and JS", // Install animations using custom CSS and JS
  "animations.Enabled": true, // Enable animations globally

  // Basic UI animations for enhanced user experience
  "basicUIAnimations.typingEffectDuration": 100, // Duration for typing effect animation (ms)
  "basicUIAnimations.smoothCaretDuration": 200, // Duration for smooth caret animation (ms)
  "basicUIAnimations.typingEffectColor": "rgba(255, 0, 0, 0.192)", // Color for typing effect
  "basicUIAnimations.typingGlowColor": "rgba(255, 0, 0, 0.6)", // Glow color for typing effect
  "basicUIAnimations.copyHighlightColor": "rgba(255, 80, 80, 0.7)", // Highlight color for copy action

  // Fluent UI settings
  "fluent-ui-vscode.accent": "#FF0000", // Set accent color to red
  "fluent-ui-vscode.compact": true, // Enable compact mode for Fluent UI
  "fluent-ui-vscode.darkBackground": "#191919", // Set dark background color for Fluent UI

  // Custom CSS imports for additional customization
  "vscode_custom_css.imports": [
    "C:/Users/cl234/.cursor/CustomCSS/script.js", // Import custom script
    "C:/Users/cl234/.cursor/CustomCSS/style.css", // Import custom stylesheet
    "file:///c:/Users/cl234/.cursor/extensions/brandonkirbyson.vscode-animations-2.0.4/dist/updateHandler.js" // Import update handler for animations
  ],

  // Spell checker settings
  "cSpell.enableFiletypes": ["!python"], // Disable spell checker for Python files

  // NoesisGUI tools settings
  "noesisgui-tools.openRenderPreviewOnStart": true, // Open render preview on start

  // HTTP experimental settings
  "http.experimental.systemCertificatesV2": true, // Enable experimental system certificate handling

  // .NET acquisition extension settings
  "dotnetAcquisitionExtension.enablePreviewFeatures": true, // Enable preview features for .NET acquisition

  // IPython notebook settings
  "ipynb.experimental.serialization": true, // Enable experimental serialization for IPython notebooks

  // MAUI configuration settings
  "maui.configuration.debugVerbosity": "diagnostic", // Set debug verbosity to diagnostic
  "maui.configuration.logVerbosity": "diagnostic", // Set log verbosity to diagnostic
  "maui.configuration.useLaunchJsonConfigurations": true, // Use launch.json configurations

  // Launch settings
  "launch": {
    "configurations": [] // Define configurations for debugging
  },

